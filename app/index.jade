  body.invisible
    #dashboardwrapper
      .backdropTop
      header
        nav#global-nav
          ul.global-nav-list
            each i in links
              li.global-nav-list-item
                if(i != 'tv & movies')
                  if(i == 'home')
                    a.global-nav-list-item-link.-active(href='##{i}')= i
                  else
                    a.global-nav-list-item-link(href='##{i}')= i
                if(i == 'tv & movies')
                  a.global-nav-list-item-link(href='#tv-movies')= i
      section
        #panels-area
          ul.global-panel-list(data-position='#home')
            each i in links
              if(i == 'bing')
                li.global-panel-list-item(id='#{i}')
                  form#bingForm.bingForm(name='bingForm', action='bing.js', method='get')
                    input(type='text', name='bingSearchTerm', placeholder='search...')
              if(i != 'tv & movies' && i != 'home' && i != 'bing' )
                li.global-panel-list-item(id='#{i}')
                  ul.tile-list
                    - for(i=1;i<=12;i++)
                      li.tile-item
              else
                if(i != 'tv & movies' && i != 'bing')
                  li.global-panel-list-item.-active(id='#{i}')
                    ul.tile-list
                      - for(i=1;i<=12;i++)
                        li.tile-item
                else
                  li.global-panel-list-item(id='tv-movies')
                    ul.tile-list
                      - for(i=1;i<=12;i++)
                        li.tile-item(tabindex='#{i}')
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js')
    script
      window.jQuery || document.write('<script src="javascripts/vendor/jquery-1.8.2.min.js"><\\/script>')


    :coffeescript
      loc = window.location.href
      $(document).ready ->

        # CLEAN URL HASH AND CANCEL FOUT
        URLindex = loc.indexOf("#")
        window.location = loc.substring(0, URLindex)  if URLindex > 0
        $("body").removeClass "invisible"

        # NAVIGATION
        $(".global-nav-list-item-link").click (e) ->
          thisID = $(this).attr("href")
          $(".global-nav-list-item-link").removeClass "-active"
          $(this).addClass "-active"
          thisID = "tv-movies"  if thisID is "tv & movies"
          $(".global-panel-list-item").removeClass("-active").filter(thisID).toggleClass "-active"
          $(".global-panel-list-item").parent().attr "data-position", thisID
          e.preventDefault()

        $(window).keypress (event) ->
          currentPanel = $(".global-panel-list").attr("data-position")
          $(".global-nav-list").find("li a[href=" + currentPanel + "]").parent().prev().find("a").trigger "click"  if event.which is 91
          $(".global-nav-list").find("li a[href=" + currentPanel + "]").parent().next().find("a").trigger "click"  if event.which is 93





